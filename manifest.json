{
  "name": "GitHub Notifier",
  "version": "0.1.0",
  "description": "Desktop Notifications for GitHub",
  "manifest_version": 3,
  "author": "Nihilok",
  "action": {
    "default_popup": "index.html",
    "default_title": "GitHub Notifier"
  },
  "permissions": [
    "tabs",
    "notifications",
    "alarms",
    "storage"
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
